<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="sarcasm" alias="origin" fetch="https://github.com/Sarcasm" pushurl="git@github.com:Sarcasm"/>

  <remote name="company-mode" alias="origin" fetch="https://github.com/company-mode" pushurl="git@github.com:Sarcasm"/>

  <remote name="emacs-lsp" alias="origin" fetch="https://github.com/emacs-lsp" pushurl="git@github.com:Sarcasm"/>
  <remote name="tigersoldier" alias="origin" fetch="https://github.com/tigersoldier" pushurl="git@github.com:Sarcasm"/>

  <remote name="cquery-project" alias="origin" fetch="https://github.com/cquery-project" pushurl="git@github.com:Sarcasm"/>

  <default remote="sarcasm" revision="master" sync-j="4"/>

  <project name=".emacs.d" path="dot-emacs"/>

  <project remote="company-mode" name="company-mode" path="company-mode"/>

  <project remote="emacs-lsp" name="lsp-mode" path="lsp-mode"/>
  <project remote="emacs-lsp" name="lsp-ui" path="lsp-ui"/>
  <project remote="tigersoldier" name="company-lsp" path="company-lsp"/>

  <project remote="cquery-project" name="cquery" path="cquery"/>
  <project remote="cquery-project" name="emacs-cquery" path="emacs-cquery"/>
</manifest>
