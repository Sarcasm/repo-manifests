<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="github" alias="origin" fetch="https://github.com/" />
  <default remote="github" revision="master" sync-j="4"/>

  <project name="Sarcasm/irony-mode" path="irony-mode"/>

  <project name="Sarcasm/flycheck-irony" path="flycheck-irony"/>
  <project name="Sarcasm/company-irony" path="company-irony"/>
  <project name="Sarcasm/ac-irony" path="ac-irony"/>

  <!--
      These could be in an "external"/third_party group
      See "Needed by" field of irony-mode in melpa:
      - https://melpa.org/#/irony
  -->
  <project name="hotpxl/company-irony-c-headers" path="external/company-irony-c-headers" groups="notdefault"/>
  <project name="ikirill/irony-eldoc" path="external/irony-eldoc" groups="notdefault"/>
  <project name="yuutayamada/company-arduino" path="external/company-arduino" groups="notdefault"/>
</manifest>
